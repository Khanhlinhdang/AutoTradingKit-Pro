QWidget {
    background-color: #d1d4dc;
}

SwitchButton {
    qproperty-spacing: 12;
    background-color: transparent;
}

SwitchButton>QLabel {
    color: black;
    font: 14px 'Segoe UI', 'Microsoft YaHei', 'PingFang SC',  'Arial';
    background-color: transparent;
    border: none;
}

SwitchButton>QLabel:disabled {
    color: rgba(0, 0, 0, 0.36);
}

